program SwapExample
  begin
    proc swap (ref a:  int, ref b : int) {
      var tmp : int;
      tmp := a;
      a := b;
      b := tmp;
  end;

  proc Main ( ) 
    begin
      var x : int;
      var y : int;
      x := 42;
      y := 84;
      swap (ref x,ref y);
      // now, x = 84 and y = 42
    end;
  end;
 